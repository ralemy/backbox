<?xml version="1.0" encoding="UTF-8"?>
<ContentPage AutomationId="TheMainPage" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="associator.Pages.MainPage">
	<ContentPage.Content>
        <StackLayout Padding = "30">
            <Button AutomationId="AssociateButton" Text="Associate" Command="{Binding GoForAssociation}"  />
            <Button AutomationId="CallApiButton" Text="Call Api" Command="{Binding CallApiCommand}"  />
        </StackLayout>
	</ContentPage.Content>
</ContentPage>
